{
  "name": "my-webpack-react-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": "luis",
  "main": "src/index.tsx",
  "sideEffects": [
    "src/**/*"
  ],
  "description": "Webpack React Typescript tutorial",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production -c ./config/webpack/prod.js",
    "build:analyzer": "npm run build -- --analyze",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "coverage": "npm run test -- --coverage",
    "dev": "npm-run-all -p mock server",
    "dev:server": "webpack --mode development",
    "dev:analyzer": "npm run dev -- --analyze",
    "format": "prettier --write 'src/**/*.+(ts|tsx|js|json|css|less|md)'",
    "mock": "echo 'start mock server'",
    "prebuild": "npm run clean",
    "start": "nodemon --exec 'npm run dev'",
    "server": "webpack serve --open --color -c ./config/webpack/dev.js",
    "watch": "webpack --watch",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx,.js --fix src",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch"
  },
